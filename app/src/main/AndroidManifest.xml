<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.internshala.kick_offassignmentsolution">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FoodRunner.Activity.DashboardActivity"></activity>
        <activity android:name=".FoodRunner.Activity.ForgotPasswordActivity" />
        <activity android:name=".FoodRunner.Activity.RegisterActivity" />
        <activity android:name=".FoodRunner.Activity.LoginActivity" />
        <activity android:name=".FoodRunner.Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>